<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_explore">
    <fragment
        android:id="@+id/navigation_explore"
        android:name="com.project.safarr.ui.explore.ExploreFragment"
        android:label="fragment_explore"
        tools:layout="@layout/fragment_explore" />
    <fragment
        android:id="@+id/navigation_travelogue"
        android:name="com.project.safarr.ui.travelogue.TravelogueFragment"
        android:label="fragment_travelogue"
        tools:layout="@layout/fragment_travelogue" />
    <fragment
        android:id="@+id/navigation_itinerary"
        android:name="com.project.safarr.ui.itinerary.ItineraryFragment"
        android:label="fragment_itinerary"
        tools:layout="@layout/fragment_itinerary" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.project.safarr.ui.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>